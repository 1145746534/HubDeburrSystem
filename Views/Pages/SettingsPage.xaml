<UserControl x:Class="HubDeburrSystem.Views.Pages.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HubDeburrSystem.Views.Pages"
             mc:Ignorable="d" 
             DataContext="{Binding Source={StaticResource Locator},Path = SettingPageViewModelLocator}"
             d:DesignHeight="850" d:DesignWidth="1550">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Assets/Styles/ButtonStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Styles/ComboBoxStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Styles/GroupBoxStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Styles/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Styles/DataGridStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="CenterAlignmentStyle" TargetType="TextBlock">
                <Setter Property="TextAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Margin="70,10,5,5">
        <Border CornerRadius="5" Background="#88D1D5E8">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <GroupBox Header="XX设置" Margin="5" Style="{StaticResource NewGroupBoxStyle}">

                    </GroupBox>
                    <GroupBox Grid.Row="1" Header="XX设置" Margin="5" Style="{StaticResource NewGroupBoxStyle}">

                    </GroupBox>
                </Grid>
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <GroupBox Header="比例尺和补偿设置" Margin="5" Style="{StaticResource NewGroupBoxStyle}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <GroupBox Header="比例尺设置" Margin="5,30,5,5" Style="{StaticResource NewGroupBoxStyle}">
                                <Grid Margin="5,10,5,5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.7*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="0.7*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="0.7*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="0.7*"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="0.6*"/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition Height="1*"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="标定板高度" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="1" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding CalibrationPlateHeight}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="标定高度 1" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="2" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding CalibrationHeight1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="物理距离 1" Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="2" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding PhysicsDistance1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="像素距离 1" Grid.Row="2" Grid.Column="4" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="2" Grid.Column="5" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding PixelDistance1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="标定高度 2" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="3" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding CalibrationHeight2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="物理距离 2" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="3" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding PhysicsDistance2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="像素距离 2" Grid.Row="3" Grid.Column="4" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="3" Grid.Column="5" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding PixelDistance2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>
                                </Grid>
                                
                            </GroupBox>
                            <GroupBox Header="机器人坐标补偿" Grid.Row="1" Margin="5" Style="{StaticResource NewGroupBoxStyle}">
                                <Grid Margin="5,10,5,5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="0.6*"/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition Height="0.8*"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Text="机器人X轴补偿" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="1" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding RobotAxisCompensateX}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="机器人Y轴补偿" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="1" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding RobotAxisCompensateY}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="机器人Z轴补偿" Grid.Row="2" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="2" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding RobotAxisCompensateZ}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>

                                    <TextBlock Text="机器人角度补偿" Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="15"/>
                                    <TextBox Grid.Row="2" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="80" Margin="10,2" 
                                             Text="{Binding RobotAngleCompensate}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Left"/>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </GroupBox>
                    <GroupBox Grid.Row="1" Header="九点标定" Margin="5" Style="{StaticResource NewGroupBoxStyle}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <GroupBox Grid.Column="1" Header="机器人坐标" Margin="5,30,5,5" Style="{StaticResource NewGroupBoxStyle}">
                                <Grid Margin="5,30,5,5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.5*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="0.5*"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="X1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX1_tbx" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY1_tbx"  Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X2" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX2_tbx" Grid.Row="1" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y2" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY2_tbx" Grid.Row="1" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X3" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX3_tbx" Grid.Row="2" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX3}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y3" Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY3_tbx" Grid.Row="2" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY3}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X4" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX4_tbx" Grid.Row="3" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX4}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y4" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY4_tbx" Grid.Row="3" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY4}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X5" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX5_tbx" Grid.Row="4" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX5}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y5" Grid.Row="4" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY5_tbx" Grid.Row="4" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY5}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X6" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX6_tbx" Grid.Row="5" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX6}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y6" Grid.Row="5" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY6_tbx" Grid.Row="5" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY6}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X7" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX7_tbx" Grid.Row="6" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX7}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y7" Grid.Row="6" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY7_tbx" Grid.Row="6" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY7}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X8" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX8_tbx" Grid.Row="7" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX8}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y8" Grid.Row="7" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY8_tbx" Grid.Row="7" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY8}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="X9" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotX9_tbx" Grid.Row="8" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotX9}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Y9" Grid.Row="8" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="RobotY9_tbx" Grid.Row="8" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="70" Margin="2,5" 
                                             Text="{Binding RobotY9}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox Grid.Column="0" Header="图像坐标" Margin="5,30,5,5" Style="{StaticResource NewGroupBoxStyle}">
                                <Grid Margin="5,30,5,5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="0.55*"/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="0.55*"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="Row1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow1_tbx" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol1_tbx"  Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol1}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row2" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow2_tbx" Grid.Row="1" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col2" Grid.Row="1" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol2_tbx" Grid.Row="1" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol2}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row3" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow3_tbx" Grid.Row="2" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow3}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col3" Grid.Row="2" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol3_tbx" Grid.Row="2" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol3}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row4" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow4_tbx" Grid.Row="3" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow4}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col4" Grid.Row="3" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol4_tbx" Grid.Row="3" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol4}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row5" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow5_tbx" Grid.Row="4" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow5}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col5" Grid.Row="4" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol5_tbx" Grid.Row="4" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol5}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row6" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow6_tbx" Grid.Row="5" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow6}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col6" Grid.Row="5" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol6_tbx" Grid.Row="5" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol6}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row7" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow7_tbx" Grid.Row="6" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow7}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col7" Grid.Row="6" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol7_tbx" Grid.Row="6" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol7}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row8" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow8_tbx" Grid.Row="7" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow8}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col8" Grid.Row="7" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol8_tbx" Grid.Row="7" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol8}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>

                                    <TextBlock Text="Row9" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageRow9_tbx" Grid.Row="8" Grid.Column="1" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageRow9}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                    <TextBlock Text="Col9" Grid.Row="8" Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="15"/>
                                    <TextBox x:Name="ImageCol9_tbx" Grid.Row="8" Grid.Column="3" Style="{StaticResource NewTextBoxStyle}" Width="69" Margin="2,5" 
                                             Text="{Binding ImageCol9}" FontSize="15" TextAlignment="Center" HorizontalAlignment="Center"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox Grid.Column="2" Header="标定和验证" Margin="5,30,5,5" Style="{StaticResource NewGroupBoxStyle}">
                                <Grid Margin="5,30,5,5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="X缩放" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding XScale}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                        <TextBlock Grid.Column="2" Text="Y缩放" HorizontalAlignment="Center" FontSize="14" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding YScale}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="R角度" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding RotationAngle}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                        <TextBlock Grid.Column="2" Text="S角度" HorizontalAlignment="Center" FontSize="14" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding SlantAngle}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                    </Grid>
                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="X位移" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding XTranslation}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                        <TextBlock Grid.Column="2" Text="Y位移" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding YTranslation}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.7*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="RMSE" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding RMSE}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                        <TextBlock Grid.Column="2" Text="MAXE" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding MAXE}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14" IsReadOnly="True"/>
                                    </Grid>
                                    <Grid Grid.Row="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Button  Content="执行九点标定" FontSize="15" VerticalAlignment="Center" Style="{StaticResource NormalButtonStyle}" Height="30" Margin="5,0,5,0"
                                         Command="{Binding CalibrationResultCommand}"/>
                                    </Grid>
                                    <Grid Grid.Row="5">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1.2*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="转换验证:" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16"/>
                                        <TextBlock Grid.Column="1" Text="高度" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="16" Margin="5,0"  Foreground="#444"/>
                                        <TextBox Grid.Column="2" Text="{Binding HubHeight}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14"/>
                                    </Grid>

                                    <Grid Grid.Row="6">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.6*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.6*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="Row" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding RowCoordinate}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14"/>
                                        <TextBlock Grid.Column="2" Text="Col" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding ColCoordinate}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14"/>
                                    </Grid>
                                    <Grid Grid.Row="7">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="0.6*"/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="0.6*"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="X" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="1" Text="{Binding XCoordinate}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14"/>
                                        <TextBlock Grid.Column="2" Text="Y" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#444"/>
                                        <TextBox Grid.Column="3" Text="{Binding YCoordinate}" Style="{StaticResource NewTextBoxStyle}" Margin="2,5" FontSize="14"/>
                                    </Grid>
                                    <Grid Grid.Row="8">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Button  Content="图像转物理" FontSize="15" VerticalAlignment="Center" Style="{StaticResource NormalButtonStyle}" Height="30" Margin="5,0,5,0"
                                         Command="{Binding PixelToPhysicsCommand}"/>
                                        <Button  Content="物理转图像" FontSize="15" Grid.Column="1" VerticalAlignment="Center" Style="{StaticResource NormalButtonStyle}" Height="30" Margin="5,0,5,0"
                                         Command="{Binding PhysicsToPixelCommand}"/>
                                    </Grid>
                                </Grid>
                            </GroupBox>
                        </Grid>
                    </GroupBox>
                </Grid>
            </Grid>
        </Border>
    </Grid>
</UserControl>
